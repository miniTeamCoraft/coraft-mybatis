<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="checkId">
        SELECT MEM_ID
            FROM MEMBERS
            WHERE MEM_ID = ?
    </entry>

    <entry key="updatePoint">
        UPDATE MEMBERS
            SET MEM_POINT = ?
            WHERE MEM_ID = ?
    </entry>
</properties>